{
	"name": "simialbi/yii2-widget-handsontable",
	"description": "yii2 handsontable widget",
	"keywords": [
		"yii2",
		"yii2-widgets",
		"handsontable",
		"table",
		"spreadsheet",
		"excel",
		"grid"
	],
	"type": "yii2-extension",
	"license": "MIT",
	"support": {
		"issues": "https://github.com/simialbi/yii2-widget-handsontable/issues",
		"source": "https://github.com/simialbi/yii2-widget-handsontable"
	},
	"authors": [
		{
			"name": "Simon Karlen",
			"email": "simi.albi@gmail.com"
		}
	],
	"repositories": [
		{
			"type": "vcs",
			"url": "git@github.com:simialbi/yii2-simialbi-base.git"
		}
	],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require": {
		"yiisoft/yii2": "~2.0",
		"yiisoft/yii2-bootstrap": "~2.0",
		"simialbi/yii2-simialbi-base": "*@dev",
		"npm-asset/handsontable": ">=0.34.5"
	},
	"config": {
		"process-timeout": 1800,
		"fxp-asset": {
			"installer-paths": {
				"npm-asset-library": "vendor/npm",
				"bower-asset-library": "vendor/bower"
			}
		}
	},
	"autoload": {
		"psr-4": {
			"simialbi\\yii2\\handsontable\\": ""
		}
	}
}